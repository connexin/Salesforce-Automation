    <validationRules>
        <fullName>Amount_Allocated_Less_Zero</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_Allocated__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Amount_Allocated__c</errorDisplayField>
        <errorMessage>Amount Allocated cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Allocated_More_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_Allocated__c &gt; Amount__c</errorConditionFormula>
        <errorDisplayField>Amount_Allocated__c</errorDisplayField>
        <errorMessage>Amount Allocated cannot be greater than Refund Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Protect_Non_Draft_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Confirmed__c == True, ISCHANGED( Amount__c ))</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>Amount cannot be changed once Refund has been Confirmed</errorMessage>
    </validationRules>
