        <type>Date</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(is_Cancelled__c, &quot;Cancelled&quot;, IF(is_Active__c, &quot;Active&quot;, &quot;Draft&quot;))</formula>
