    </validationRules>
    <validationRules>
        <fullName>Valid_MAC_Address</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(Active__c = false, MAC_Address__c = &quot;&quot;),AND( Active__c = true,
AND(NOT(REGEX( MAC_Address__c , &quot;(([A-F]|[0-9]){2}:){5}([A-F]|[0-9]){2}&quot;)), MAC_Address__c != &quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>MAC_Address__c</errorDisplayField>
        <errorMessage>Invalid MAC Address</errorMessage>
    </validationRules>
