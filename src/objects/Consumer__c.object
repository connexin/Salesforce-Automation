    <validationRules>
        <fullName>isValidPostcode</fullName>
        <active>true</active>
        <description>Valid UK Postcode.</description>
        <errorConditionFormula>NOT(
	OR(
		ISBLANK( Postcode__c ),
		REGEX( Postcode__c , &quot;[a-zA-Z]\\d\\s\\d[a-zA-Z]{2}&quot;),
		REGEX( Postcode__c , &quot;[a-zA-Z]\\d\\d\\s\\d[a-zA-Z]{2}&quot;),
		REGEX( Postcode__c , &quot;[a-zA-Z]{2}\\d\\s\\d[a-zA-Z]{2}&quot;),
