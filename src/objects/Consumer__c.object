		REGEX( Postcode__c , &quot;[a-zA-Z]{2}\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;),
		REGEX( Postcode__c , &quot;[a-zA-Z]\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;)
	)
)</errorConditionFormula>
        <errorDisplayField>Postcode__c</errorDisplayField>
        <errorMessage>The postcode must be valid UK format of the form XX99 9XX</errorMessage>
    </validationRules>
