    <validationRules>
        <fullName>AOV_Equal_TCV_when_Contract_Length_12</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Contract_Length__c &lt;= 12,  Year_1_AOV__c != Amount)</errorConditionFormula>
        <errorDisplayField>Year_1_AOV__c</errorDisplayField>
        <errorMessage>Year 1 AOV must equal TCV if the Contract Length is less than 12 Months</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AOV_Less_Than_TCV</fullName>
        <active>true</active>
        <errorConditionFormula>Year_1_AOV__c &gt; Amount</errorConditionFormula>
        <errorDisplayField>Year_1_AOV__c</errorDisplayField>
        <errorMessage>Year 1 AOV cannot be more than Opportunity TCV</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Close_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IsClosed, ISBLANK( Close_Reason__c ))</errorConditionFormula>
        <errorDisplayField>Close_Reason__c</errorDisplayField>
        <errorMessage>Close Reason must be specified when Opportunity is Closed</errorMessage>
    </validationRules>
