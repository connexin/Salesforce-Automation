<apex:page standardController="Consumer__c" recordSetVar="consumers">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockTable value="{!Consumers}" var="record">
                <apex:column headerValue="Customer_State__c">
                    <apex:outputField value="{!record.Customer_State__c}"/>
                </apex:column>
                <apex:column headerValue="Name">
                    <apex:outputField value="{!record.name}"/>
                </apex:column>
                <apex:column headerValue="Customer_Name__c">
                    <apex:outputField value="{!record.Customer_Name__c}"/>
                </apex:column>
                <apex:column headerValue="Postcode__c">
                    <apex:outputField value="{!record.Postcode__c}"/>
                </apex:column>
                <apex:column headerValue="Product__c">
                    <apex:outputField value="{!record.Product__c}"/>
                </apex:column>
                <apex:column headerValue="Contract_Term__c">
                    <apex:outputField value="{!record.Contract_Term__c}"/>
                </apex:column>
                <apex:column headerValue="Coverage_Check_Result__c">
                    <apex:outputField value="{!record.Coverage_Check_Result__c}"/>
                </apex:column>
                <apex:column headerValue="Credit_Check_Result__c">
                    <apex:outputField value="{!record.Credit_Check_Result__c}"/>
                </apex:column>                
            </apex:pageBlockTable>
            <apex:pageBlockButtons >
                <apex:commandButton value="First" action="{!first}"/>
                <apex:commandButton value="Previous" action="{!previous}"/>
                <apex:commandButton value="Next" action="{!next}"/>
                <apex:commandButton value="Last" action="{!last}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>